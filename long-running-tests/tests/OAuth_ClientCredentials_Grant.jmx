client_secret = &quot;${consumer_secret}&quot;;
client_id = &quot;${consumer_key}&quot;;
redirect_uri = &quot;${callback_url_prefix}${sp_count_id}&quot;;
tokenEndpoint = &quot;/oauth2/token&quot;;

}

